{
  "name": "@radar/lnrpc",
  "version": "v0.8.0-beta.3",
  "description": "typed gRPC client for lightningnetwork/lnd",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "config": {
    "lnd-release-tag": "v0.8.0-beta",
    "lnd-url": "https://raw.githubusercontent.com/lightningnetwork/lnd",
    "protoc-version": "3.5.1"
  },
  "scripts": {
    "test": "mocha \"test/**/*.test.ts\"",
    "start": "ts-node ./src/index.ts",
    "build": "tsc -p tsconfig.build.json",
    "lint": "tslint -c tslint.json '{src,test}/**/*.ts' -e 'src/types/generated/**/*.ts' --fix",
    "generate": "sh ./generate_types.sh ${npm_package_config_lnd_release_tag} ${npm_package_config_protoc_version} && ts-node src/clean-repeated.ts",
    "update-proto": "mkdir -p lnd/${npm_package_config_lnd_release_tag} && curl -o lnd/${npm_package_config_lnd_release_tag}/rpc.proto ${npm_package_config_lnd_url}/${npm_package_config_lnd_release_tag}/lnrpc/rpc.proto"
  },
  "repository": "https://github.com/RadarTech/lnrpc",
  "keywords": [
    "lnd",
    "lightning network",
    "grpc",
    "client",
    "bitcoin"
  ],
  "author": "RadarTech",
  "license": "MIT",
  "dependencies": {
    "@grpc/proto-loader": "^0.4.0",
    "@types/google-protobuf": "^3.2.7",
    "grpc": "^1.19.0",
    "pkg-dir": "^2.0.0",
    "protobufjs": "^6.8.8",
    "ts-protoc-gen": "^0.8.0"
  },
  "devDependencies": {
    "@types/node": "^10.14.19",
    "@types/pkg-dir": "^2.0.1",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "ts-node": "^8.4.1",
    "tslint": "^5.17.0",
    "typescript": "^3.6.3"
  },
  "engines": {
    "node": ">= 8.*"
  }
}
